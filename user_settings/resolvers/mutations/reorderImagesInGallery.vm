{
    "version" : "2017-02-28",
    "operation" : "UpdateItem",
    "key" : {
        "id" : { "S" : "$ctx.args.id" },
        "type" : { "S" : "gallery" }
    },
    "update" : {
       "expression" : "SET images = :new_images",
       "expressionValues" : {
           ":new_images" : $util.dynamodb.toDynamoDBJson($ctx.args.image_ids)
       }
    }
}
